<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Test_Select">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/select_usim_puf"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="SELECT  USIM PUF" />

        <Button
            android:id="@+id/select_init"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="USIM PUF PW 초기화" />
        <Button
            android:id="@+id/select_test_etc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="SELECT ETC" />

        <TextView
            android:id="@+id/sample_result"
            android:layout_width="match_parent"
            android:layout_height="147dp"
            android:layout_gravity="center_horizontal"
            android:text="RESULT" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>