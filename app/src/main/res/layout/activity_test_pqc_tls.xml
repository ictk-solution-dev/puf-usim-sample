<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Test_PqcTls">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/key_pqc_tls_get_build_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="BUILD DATE" />
        <Button
            android:id="@+id/key_pqc_tls_snd_recv_sample"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="PQC TLS SEND N RECV SAMPLE" />
        <Button
            android:id="@+id/key_pqc_tls_kem_test"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="KEM TEST" />

        <TextView
            android:id="@+id/sample_result"
            android:layout_width="match_parent"
            android:layout_height="147dp"
            android:layout_gravity="center_horizontal"
            android:text="RESULT" />



    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>